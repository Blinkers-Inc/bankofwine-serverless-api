    "db:pull": "dotenv -e .env.dev -- npx prisma db pull",
    "db:push:dev": "dotenv -e .env.dev -- npx prisma db push",
    "pre:install": "npx prisma generate",
    "dev": "SLS_DEBUG=* rm -rf .serverless & npx sls offline --stage dev",
